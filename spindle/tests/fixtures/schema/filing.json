{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "https://schema.occrp.org/generic/filing.json#",
  "title": "Filing",
  "description": "A statutory filing",
  "type": "object",
  "properties": {
    "title": {
      "type": "string"
    },
    "date": {
      "type": "string",
      "format": "date"
    },
    "description": {
      "type": "string"
    },
    "uid": {
      "type": "string"
    },
    "url": {
      "type": "string",
      "format": "uri"
    },
    "filing_type_code": {
      "type": "string"
    },
    "filing_type_name": {
      "type": "string"
    },
    "other_attributes": {
      "type": "object"
    }
  },
  "required": [
    "date"
  ],
  "anyOf": [
    {
      "required": ["title"]
    },
    {
      "required": ["description"]
    },
    {
      "required": ["filing_type_name"]
    }
  ]
}
